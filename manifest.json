{
	"manifest_version": 2,
	"version": "0.0.4",
	"name": "webcleaner",
	"description": "理清网页",
	"icons": {
		"48": "icons/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/*"
			],
			"js": [
				"content_scripts/content_common.js",
				"lib/jquery-3.6.0.min.js"
			]
		},
		{
			"matches": [
				"*://www.baidu.com/*"
			],
			"js": [
				"content_scripts/baidu/content_baidu.js"
			]
		},
		{
			"matches": [
				"*://blog.csdn.net/*"
			],
			"js": [
				"content_scripts/csdn/content_csdn.js"
			]
		}
	],
	"browser_action": {
		"default_icon": {
			"48": "icons/icon.png"
		},
		"default_title": "清理网页",
		"default_popup": "browserAction/index.html"
	},
	"permissions": [
		"webRequest",
		"<all_urls>",
		"activeTab",
		"contextMenus"
	],
	"background": {
		"scripts": [
			"background/background.js"
		]
	}
}