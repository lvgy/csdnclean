{
	"manifest_version": 2,
	"version": "0.0.2",
	"name": "csdnclean",
	"description": "理清csd网页",
	"icons": {
		"64": "icons/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://blog.csdn.net/*",
				"*://www.baidu.com/*"
			],
			"js": [
				"content_scripts/content_script.js",
				"lib/jquery-3.6.0.min.js"
			]
		}
	],
	"browser_action": {
		"default_icon": {
			"48": "icons/icon.png"
		},
		"default_title": "清理网页",
		"default_popup": "browserAction/index.html"
	}
}